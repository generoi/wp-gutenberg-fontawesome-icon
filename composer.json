{
  "name": "generoi/wp-gutenberg-fontawesome-icon",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "FontAwesome Icon for Gutenberg",
  "homepage": "https://github.com/generoi/wp-gutenberg-fontawesome-icon",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "keywords": [
    "wordpress",
    "gutenberg"
  ],
  "support": {
    "issues": "https://github.com/generoi/wp-gutenberg-fontawesome-icon/issues"
  },
  "repositories": [
